<h1>Listing feed_entries</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Summary</th>
    <th>Url</th>
    <th>Published at</th>
    <th>Guid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feed_entries.each do |feed_entry| %>
  <tr>
    <td><%= feed_entry.title %></td>
    <td><%= feed_entry.summary %></td>
    <td><%= feed_entry.url %></td>
    <td><%= feed_entry.published_at %></td>
    <td><%= feed_entry.guid %></td>
    <td><%= link_to 'Show', feed_entry %></td>
    <td><%= link_to 'Edit', edit_feed_entry_path(feed_entry) %></td>
    <td><%= link_to 'Destroy', feed_entry, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed entry', new_feed_entry_path %>
